		group="sdkEx" handlerType="div" icon="res/icon.png">
		<jsInclude>res/js/properties.js</jsInclude>
		<cssInclude>res/css/properties.css</cssInclude>
		<property type="Color" title="Color" id="color"></property>
		<property type="int" title="Border Width" id="borderWidth"></property>
		<initialization>
			<defaultValue property="color">#FF0000</defaultValue>
			<defaultValue property="WIDTH">500</defaultValue>
			<defaultValue property="HEIGHT">300</defaultValue>
			<defaultValue property="TOP_MARGIN">0</defaultValue>
			<defaultValue property="LEFT_MARGIN">0</defaultValue>
			<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
			<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		</initialization>
